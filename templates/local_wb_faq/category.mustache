{{!
    @template mod_mooduell/viewpage
    Template which defines a mooduell view page.
    Classes required for JS:
    * none
    Data attributes required for JS:
    * none
    Context variables required for this template:
    Example context (json):
    {
        "id": "5",
        "name": "asdasfd",
        "description": "asd",
        "type": "category",
        "timecreated": "1638390631",
        "timemodified": "1638390629",
        "openentity": "0",
        "createdby": "2",
        "picture": null,
        "parentid": "0",
        "sortorder": null,
        "address": [
            null
        ],
        "contacts": [
            null
        ]
    }
}}


<li class="btn px-0 d-block text-left "
        data-action="goto"
        data-targetid="{{id}}"
        data-id="{{id}}"
        data-sortorder="{{sortorder}}"
        data-module="{{module}}"
        data-supplement="{{supplement}}">
    {{#canedit}}
    <span tabindex="0" role="button" aria-haspopup="true" data-drag-type="move" title="{{title}}">
        <i class="fa fa-arrows"></i>
    </span>
    {{/canedit}}
    <h4 class="lexa-h4" data-action="goto"
        data-targetid="{{id}}"
        data-id="{{id}}"
        data-sortorder="{{sortorder}}"
        data-module="{{module}}"
        data-supplement="{{supplement}}">
    {{title}}
    </h4>
    {{#canedit}}
        <span class="wb_faq_edit_entry_elements">
            {{#enabled}}
              <a class="local_wb_faq_toggle_category_visibility" data-id="{{id}}"><i class="toggle-visibility fa fa-eye
               mr-3"></i></a>
               {{/enabled}}
               {{^enabled}}
              <a class="local_wb_faq_toggle_category_visibility" data-id="{{id}}"><i class="toggle-visibility fa fa-eye-slash
               mr-3"></i></a>
               {{/enabled}}
            <a class="local_wb_faq_edit_category" data-id="{{id}}"><i class="fa fa-pencil mr-3"></i></a>
            <a class="local_wb_faq_delete_category" data-id="{{id}}"><i class="fa fa-trash-o mr-3"></i></a>
        </span>
    {{/canedit}}
</li>
